{
    "name": "devianl2/tenant-auth",
    "description": "Microservice auth package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Devian Leong",
            "email": "devian@peoplelogy.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0|^9.0",
        "ext-openssl": "*",
        "lcobucci/jwt": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Tenant\\Auth\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenant\\Auth\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tenant\\Auth\\TenantAuthProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
